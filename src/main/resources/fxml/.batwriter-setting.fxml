<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.azarenka.batwriter.controllers.SettingsController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="edit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="28.0" layoutY="38.0" text="Directory Name:" />
            <Button fx:id="buttonAdd" layoutX="114.0" layoutY="325.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Изменить" />
            <Button layoutX="139.0" layoutY="34.0" mnemonicParsing="false"  prefHeight="25.0" prefWidth="27.0" text="choiceButton" />
            <Separator layoutX="397.0" layoutY="14.0" orientation="VERTICAL" prefHeight="343.0" prefWidth="0.0" />
            <Label fx:id="pathLabel" layoutX="28.0" layoutY="59.0" prefHeight="17.0" prefWidth="365.0" />
            <Label layoutX="28.0" layoutY="85.0" text="File Name" />
            <Button layoutX="139.0" layoutY="81.0" mnemonicParsing="false" onAction="#choiceFile" prefHeight="25.0" prefWidth="27.0" text="choiceButton" />
            <Label fx:id="pathFileLabel" layoutX="28.0" layoutY="119.0" prefHeight="17.0" prefWidth="365.0" />
            <Button layoutX="333.0" layoutY="14.0" mnemonicParsing="false" text="Главная" onAction="#setMainWindowScene" />
         </children>
    </AnchorPane>
  </children>
</VBox>
